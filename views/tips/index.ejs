<ul>
    <% for (var i in tips) { %>
    <% var tip = tips[i]; %>
    <li>
        <% if (tip.accepted ||
                (session.user && (session.user.isAdmin || (tip.Author.id && tip.Author.id === session.user.id)))) { %>
        <%= tip.text %>
        <% } %>
        <small>
            (by <span id="autor"><%= quiz.Tips[i].Author.username ? quiz.Tips[i].Author.username : "AnÃ³nimo" %></span>)
        </small>
        <% if (!tip.accepted &&
                (session.user && (tip.Author.id && tip.Author.id === session.user.id))) { %>
        <small> (No se ha publicado) </small>
        <% } %>
        <% if (!tip.accepted &&
                (session.user && (session.user.isAdmin || (quizAuthorId && quizAuthorId === session.user.id)))) { %>
        <a href="/quizzes/<%= quizId %>/tips/<%= tip.id %>/accept?_method=PUT"><button>Aceptar</button></a>
        <% } %>
        <% if ((session.user && (session.user.isAdmin || (tip.Author.id && tip.Author.id === session.user.id)))) { %>
        <a href="/quizzes/<%= quizId %>/tips/<%= tip.id %>?_method=DELETE"><button>Borrar</button></a>
        <% } %>
    </li>
    <% } %>
</ul>